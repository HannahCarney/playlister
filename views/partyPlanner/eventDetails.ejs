<section class="center jumbotron">
  <h1>Event Info</h1>
  <h2>A better way to play music</h2>
  <form action="/pp/event" method="post" id="party-event-form">
    <label for="eventName">Party name:</label>
    <input name="eventName" id="eventName">
    <label for="eventPlaylist">Party playlist name:</label>
    <input name="eventPlaylist" id="eventPlaylist">
     <label for="eventDate">Date:</label>
    <input type="date" name="eventDate" id="eventDate">
    <input type="submit" value="Save">
  </form>
</section>
